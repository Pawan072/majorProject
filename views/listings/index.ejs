<% layout("/layouts/boilerplate") %>
<body>
    <h3>All Listing</h3>
    <form method="GET" action="/listings/new">
        <button>Create New listing</button>
    </form>
    <ul>
        <% for(Listing of allListing) { %>
            <li>
                <a href="/listing/<%= Listing._id %>"><%= Listing.title %></a>
            </li>
        <% } %>
    </ul>
</body>

